@font-face {
  font-family: 'Open Sans';
  font-style: normal;
  font-weight: 300;
  src: local('Open Sans Light'),
    local('OpenSans-Light'),
    url(/fonts/opensans-light-webfont.woff2) format('woff2')
    url(/fonts/opensans-light-webfont.woff) format('woff');
}
